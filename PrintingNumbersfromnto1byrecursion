#include <vector>
#include <iostream>
#include <string>
#include <map>
#include <numeric>
using namespace std;

void print(int n,int k)
{
    if(n==0)
    return;
    
    print(n-1,k);
    cout<<(k-n+1)<<" ";
}

int main()
{
    int n;
    cin>>n;
    //printing of numbers by recursion 
    int k=n;
    print(n,k);
}
